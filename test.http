GET http://localhost:4000/user/me
authorization: Bearer 1eed05c6-4377-69f0-5c35-6891ac906ab5

# GET 요청: 한줄 외치기 모두 조회
###
GET http://localhost:4000/shout/all_messages

# POST 요청: 한줄 외치기 쓰기
###
POST http://localhost:4000/shout/add
authorization: Bearer 1234
Content-Type: application/json

{
    "content": "오 이 채팅 기능 대박이다!!",
    "emoji": "happy!"    
}

