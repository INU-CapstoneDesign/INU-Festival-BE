# POST 요청: 관리자 부스 추가
###
POST http://localhost:4000/admin/booth
Content-Type: application/json

{
    "name": "test1",
    "category": "비주점",
    "department": "희희낙락",
    "description": "test1",
    "liked": 0
}

# PUT 요청: 좋아요 추가
###
PUT http://localhost:4000/booth/liked/1
Content-Type: application/json

{
    "likeCount": 22
}

# POST 요청: 부스 댓글 추가
###
POST http://localhost:4000/booth/comment/1
authorization: Bearer 1eed0068-e169-6590-c2bf-8f82fb5eaed1
Content-Type: application/json

{
    "content": "새로운댓글1",
    "emoji": "happy",
    "boothId": 1
}

